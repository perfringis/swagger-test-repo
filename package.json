{
  "name": "swagger-test-repo",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:perfringis/swagger-test-repo.git",
  "author": "dzidzi <kopko.mateusz@gmail.com>",
  "license": "MIT",
    "scripts": {
    "clean:dist": "rm -rf dist",
    "build:prod": "tsc",
    "start:dev": "ts-node src",
    "start:prod": "node ./dist/src",
    "start": "yarn run clean:dist && yarn run swagger && yarn run build:prod && yarn run start:prod",
    "nodemon": "yarn run swagger && nodemon",
    "nodemon:debug": "nodemon --inspect=0.0.0.0:9229 index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"spec/**/*.ts\"",
    "lint": "eslint \"{src,tests,e2e}/**/*.ts\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:e2e": "jest --config e2e.config.ts",
    "swagger": "swaggerGen -c ./swaggerConfig.json"
  },
    "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^19.7.2",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.12",
    "typescript-ioc": "^3.2.2",
    "typescript-rest": "^3.0.4",
    "typescript-rest-ioc": "^1.0.1",
    "typescript-rest-swagger": "^1.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
